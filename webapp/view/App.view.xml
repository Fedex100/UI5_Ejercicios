<mvc:View
    controllerName="ejer18.ejer18.controller.App"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    displayBlock="true"
    xmlns="sap.m"
>
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content> 
                    <core:Fragment fragmentName="ejer18.ejer18.fragments.IconTabBar"/>
                    <core:Fragment fragmentName="ejer18.ejer18.fragments.IconTabBar_2"/>
                    <core:Fragment fragmentName="ejer18.ejer18.fragments.IconTabBar_3"/>
                   
                    </content>    
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
